<?xml version="1.0" encoding="ISO-8859-1"?>
<node id="top">
  <node id="FIFO" address="0x0"  mode="non-incremenal"  permission="r"
        description="FIFO to store the tracking data coming from the VFATs connected to the OptoHybrid"/>
  <node id="FLUSH" address="0x0"  mask="0x1"  permission="w"
        description="Flush the FIFO by writing a 1 to it"/>
  
  <node id="DEPTH" address="0x1"  permission="r"
        description="FIFO depth in number of 32 bit words"/>
  <node id="ISFULL"   address="0x2"  mask="0x1"  permission="r"
        description="Is the FIFO full"/>
  <node id="ISEMPTY"  address="0x3"  mask="0x1"  permission="r"
        description="Is the FIFO empty"/>
</node> <!-- end top block -->
